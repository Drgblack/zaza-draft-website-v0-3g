{
    "compilerOptions":  {
                            "target":  "ES2022",
                            "lib":  [
                                        "dom",
                                        "dom.iterable",
                                        "esnext"
                                    ],
                            "allowJs":  true,
                            "skipLibCheck":  true,
                            "strict":  false,
                            "noEmit":  true,
                            "esModuleInterop":  true,
                            "module":  "esnext",
                            "moduleResolution":  "bundler",
                            "resolveJsonModule":  true,
                            "isolatedModules":  true,
                            "jsx":  "preserve",
                            "plugins":  [
                                            {
                                                "name":  "next"
                                            }
                                        ],
                            "incremental":  true,
                            "forceConsistentCasingInFileNames":  true,
                            "types":  [

                                      ],
                            "baseUrl":  ".",
                            "paths":  {

                                      }
                        },
    "include":  [
                    "next-env.d.ts",
                    "**/*.ts",
                    "**/*.tsx",
                    "**/*.mts",
                    "**/*.cts"
                ],
    "exclude":  [
                    "node_modules"
                ]
}